<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:Contoso.Forms.Test;assembly=Contoso.Forms.Test" x:Class="Contoso.Forms.Test.TestPage">
	<TableView Intent="Form">
		<TableSection>
			<ViewCell>
				<Button AutomationId="{x:Static local:TestStrings.GoToTogglePageButton}" Text="Go to Toggle Page" Clicked="GoToTogglePage" />
			</ViewCell>
			<ViewCell>
				<Button AutomationId="{x:Static local:TestStrings.GoToCrashesPageButton}" Text="Go to Crashes Page" Clicked="GoToCrashesPage" />
			</ViewCell>
			<ViewCell>
				<Button AutomationId="{x:Static local:TestStrings.GoToAnalyticsPageButton}" Text="Go to Analytics Page" Clicked="GoToAnalyticsPage" />
			</ViewCell>
			<ViewCell>
                <Button AutomationId="{x:Static local:TestStrings.GoToCrashResultsPageButton}" Text="Go to Crash Results Page" Clicked="GoToCrashResultsPage" />
            </ViewCell>
           <ViewCell>
                <Button AutomationId="{x:Static local:TestStrings.GoToCorePageButton}" Text="Go to Core Page" Clicked="GoToCorePage" />
            </ViewCell>
		</TableSection>
	</TableView>
</ContentPage>